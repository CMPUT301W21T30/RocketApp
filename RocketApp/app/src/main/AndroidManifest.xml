<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rocketapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RocketApp">
        <activity android:name=".ExperimentQuestionsActivity"></activity>
        <activity
            android:name=".OwnerActivity"
            android:parentActivityName=".ExperimentsListActivity" />
        <activity
            android:name=".ExperimentActivity"
            android:parentActivityName=".ExperimentsListActivity" />
        <activity
            android:name=".AllExperiments"
            android:parentActivityName=".ExperimentsListActivity" />
        <activity
            android:name=".LogInActivity"
            android:parentActivityName=".ExperimentsListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserProfileActivity"
            android:parentActivityName=".ExperimentsListActivity" />
        <activity
            android:name=".ExperimentsListActivity"
            android:parentActivityName=".LogInActivity" />
    </application>

</manifest>